$(document).ready(function() {
var brandId,brandColor;
$.ajax({
    url: "product.json",
    type: "GET",
    dataType: "json",
    success: function(data){
    jsonData = data;        
    //alert(jsonData.val());
   alert(jsonData.length);
 for(i = 0;i < jsonData.length;i = i + 1)
      {
        url = 'images/' + jsonData[i].url;
        brandId = jsonData[i].brand;
        brandColor = jsonData[i].color;
    
        $('#outerDiv').append('<img></img>').attr('src',url).attr('sold_out',jsonData[i]["sold_out"]);

 }}
  });
});
